    "brunch": "2.10.9",
    "clean-css-brunch": "2.10.0",
    "uglify-js-brunch": "2.10.0",
    "sass-brunch": "2.10.7",
    "elm": "0.18.0",
    "elm-brunch": "0.10.0",
    "elm-format": "0.6.1-alpha",
    "elm-github-install": "1.6.1"
  }
}
